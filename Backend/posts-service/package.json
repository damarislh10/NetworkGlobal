{
  "name": "posts-service",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "test": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest --watch",
    "test:cov": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest --coverage",
    "seed": "tsx seed.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.14",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
